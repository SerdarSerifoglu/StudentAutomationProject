@model StudentAutomationProject.Models.Security.RegisterViewModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Kayıt Ol</title>
</head>
<body>
    <h3>Kayıt Ol</h3>
    <form asp-controller="Security" asp-action="Register" method="post">
        <div asp-validation-summary="All"></div>
        <input type="text" asp-for="Username" placeholder="Kullanıcı Adı" /><br />
        <input type="password" asp-for="Password" placeholder="Şifre" /><br />
        <input type="password" asp-for="ConfirmedPassword" placeholder="Şifre Tekrar" /><br />
        <input type="text" asp-for="Email" placeholder="Email" /><br />
        <input type="submit" value="Kayıt Ol" /><br />
    </form>
</body>
</html>



